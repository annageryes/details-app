pipeline{
    agent {label 'workers'}

    stages{
        
        stage('spellcheck'){
            steps{
                echo 'check code spell check'
                sleep 2
                sh '''
                echo "pip install codespell"
	            echo "spellcheck /home/jenkins/workspace/spellcheck/"
                    #codespell /home/jenkins/
                '''
            } //error with artifact
        }
    }
}


